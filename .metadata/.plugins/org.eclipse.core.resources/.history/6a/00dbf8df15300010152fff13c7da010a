#include "flash_interface.h"

void flash_write(uint32_t addr, char* data, size_t n){

	__disable_irq();
	FLASH->KEYR = 0x45670123;
	FLASH->KEYR = 0xCDEF89AB;



	FLASH->CR |= FLASH_CR_LOCK;

}



void flash_read(uint32_t addr, char* data, size_t n){



}
